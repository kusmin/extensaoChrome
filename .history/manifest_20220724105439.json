{
  "name": "Extensão para alterar pagina inicial",
  "description": "Extensão que altera pagina inicial",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "chrome_settings_overrides": {
    "homepage": "http://www.homepage.com"
  }
}